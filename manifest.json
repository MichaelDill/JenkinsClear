{
    "name": "SuperClear",
    "version": "1.0.0",
    "description": "Clear all those pre-filled jenkins fields",
    "manifest_version": 3,
    "author": "Michael DIll",
	"content_scripts": [
		{
		  "matches": ["<all_urls>"],
		  "js": ["content.js"]
		}
	],
    "permissions": [
        "activeTab"
      ],
    "action":{
        "default_popup": "index.html",
        "default_title": "Clear it"
    }
}